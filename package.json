{
    "name": "project-bot-monorepo",
    "private": true,
    "scripts": {
    "dev": "pnpm --filter \"./apps/*\" --parallel dev",
    "build": "pnpm --filter \"./apps/*\" build",
    "lint": "pnpm --filter \"./apps/*\" --filter \"./packages/*\" lint",
    "test": "pnpm -r --filter \"./apps/*\" test",
    "test:debug": "vitest run",
    "test:affected": "pnpm -r --filter \"...[origin/main]\" test",
    "test:e2e": "pnpm --filter e2e-tests e2e",
    "audit": "pnpm audit --audit-level moderate",
    "optimize-assets": "node tooling/optimize-images.js"
},
    "devDependencies": {
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "^16.3.2",
        "happy-dom": "^20.4.0",
        "sharp": "^0.34.5",
        "typescript": "^5.0.0",
        "vite": "^5.0.0",
        "vitest": "^4.0.18"
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "@swc/core",
            "better-sqlite3",
            "esbuild",
            "sharp"
        ],
        "overrides": {
            "qs": "^6.14.1",
            "tar": "^7.5.7",
            "lodash": "^4.17.23",
            "undici": "^6.23.0",
            "vite@^5": "^5.4.21",
            "koa": "^2.16.2",
            "tmp": "^0.2.4",
            "ai": "^5.0.52",
            "glob": "^10.5.0",
            "esbuild": "^0.25.0"
        }
    }
}