{
  "kind": "collectionType",
  "collectionName": "questions",
  "info": {
    "singularName": "question",
    "pluralName": "questions",
    "displayName": "Question"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "question_id": {
      "type": "string",
      "minLength": 6,
      "maxLength": 6,
      "required": true,
      "unique": true
    },
    "text": {
      "type": "string",
      "required": true
    },
    "Option": {
      "type": "component",
      "component": "default.option",
      "repeatable": true,
      "required": true
    },
    "default_next_question": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::question.question"
    },
    "tooltip": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::tooltip.tooltip"
    }
  }
}
